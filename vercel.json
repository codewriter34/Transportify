{
  "functions": {
    "api/basic.cjs": {
      "runtime": "nodejs18.x"
    },
    "api/hello.cjs": {
      "runtime": "nodejs18.x"
    },
    "api/index.cjs": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/basic",
      "dest": "/api/basic.cjs"
    },
    {
      "src": "/hello",
      "dest": "/api/hello.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

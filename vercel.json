{
  "version": 2,
  "functions": {
    "api/track.js": {
      "runtime": "nodejs18.x"
    },
    "api/test.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/track/(.*)",
      "dest": "/api/track.js"
    },
    {
      "src": "/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install"
}

{
  "version": 2,
  "builds": [
    {
      "src": "admin-dashboard/server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["admin-dashboard/server/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "admin-dashboard/server/server.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "admin-dashboard/server/server.js": {
      "maxDuration": 30
    }
  }
}
